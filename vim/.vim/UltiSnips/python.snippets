# override existing snippets from UltiSnips
priority 1

snippet logger "logger" b
import logging

logging.basicConfig(
	level=logging.INFO,
	format="[%(asctime)s] [%(levelname)-8s] [%(module)s %(name)s (%(lineno)d)] : %(message)s",
	datefmt="%Y-%m-%d %H:%M:%S %z",
)
logger = logging.getLogger(__name__)


$0
endsnippet

snippet shebang "shebang" b
#!/usr/bin/env python3

$0
endsnippet

snippet bp "breakpoint" b
breakpoint()$0
endsnippet
