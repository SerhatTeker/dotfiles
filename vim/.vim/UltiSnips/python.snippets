# override existing snippets from UltiSnips
priority 1

snippet logger "logger" b
import logging

logging.basicConfig(
	level=logging.DEBUG,
	format="[%(asctime)s] %(levelname)-9s [%(module)s %(name)s (%(lineno)d)] : %(message)s",
	datefmt="%Y-%m-%d %H:%M:%S",
)
logger = logging.getLogger(__name__)


$0
endsnippet
