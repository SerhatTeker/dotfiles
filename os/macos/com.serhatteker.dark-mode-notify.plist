<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.serhatteker.dark-mode-notify</string>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/.dark-mode-notify-stderr.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/.dark-mode-notify-stdout.log</string>
    <key>ProgramArguments</key>
    <array>
       <string>/usr/local/bin/dark-mode-notify</string>
       <string>/bin/bash</string>
       <string>-c</string>
       <string>${HOME}/.local/bin/adapt_term_bg_macos</string>
    </array>
</dict>
</plist>
